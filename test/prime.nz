i32 main(i32 argc) {
    i32 i = 0;
    i32 j;
    while (i < 100) {
        i = i + 1;
        j = 3;
        if (i % 2 == 0) {
            printf("%d is not prime", i);
            continue;
        }
        i32 is_prime = 1;
        while (j < i) {
            if (i % j == 0) {
                is_prime = 0;
                break;
            }
            j = j + 1;
        }
        if (is_prime == 1) {
            printf("%d is prime", i);
        } else {
            printf("%d is not prime", i);
        } 
    }
    return 0;
}

